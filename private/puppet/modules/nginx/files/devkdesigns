server {
	listen 80;

	server_name _;
  #server_name devkdesigns.com www.devkdesigns.com

	root /srv/devkdesigns.com/public;
	index index.php;

	location / {
		try_files $uri /index.php;
	}

	location ~ \.php$ {
		fastcgi_pass 127.0.0.1:9000;
		fastcgi_index index.php;
		include fastcgi_params;
	}
}
